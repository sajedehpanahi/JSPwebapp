<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>RealCustomerController</servlet-name>
        <servlet-class>presentation.RealCustomerController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoanTypeController</servlet-name>
        <servlet-class>domainLogic.LoanTypeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GrantConditionController</servlet-name>
        <servlet-class>domainLogic.GrantConditionController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoanFileController</servlet-name>
        <servlet-class>domainLogic.LoanFileController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RealCustomerController</servlet-name>
        <url-pattern>/RealCustomerController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoanTypeController</servlet-name>
        <url-pattern>/LoanTypeController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GrantConditionController</servlet-name>
        <url-pattern>/GrantConditionController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoanFileController</servlet-name>
        <url-pattern>/LoanFileController</url-pattern>
    </servlet-mapping>
</web-app>
